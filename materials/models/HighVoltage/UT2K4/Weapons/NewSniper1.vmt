	"$envmap" "env_cubemap"
	"$envmapfrensel" "1"
	"$envmaptint" "[.4 .4 .4]"
	"$basealphaenvmapmask" 1

	"360?$color2" "[ 0.6 0.6 0.6 ]"
}
