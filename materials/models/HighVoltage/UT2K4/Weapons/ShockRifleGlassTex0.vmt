 "UnlitGeneric"
{
	"$basetexture" "models/HighVoltage/UT2K4/Weapons/ShockRifleGlassTex0"
	"$translucent" "1"

	"$envmap" "env_cubemap"
	"$envmapfrensel" "1"
	"$envmaptint" "[.2 .2 .2]"

	"$model" 1

    	"$angle" 0.0
    	"$translate" "[1.0 1.0]"
    	"$center" "[-0.5 -0.5]"
	"$scale" "[1.0 1.0]"
	"$offset" "0"

    "Proxies"
    {
	"EntityRandom"
        	{
            		"scale" "12"
            		"resultVar" "$offset"
	}
	"Sine"
        	{
            		"sineperiod" 6
            		"timeoffset" "$offset"	       
		"sinemin" "0.9"
		"sinemax" "1.1"
            		"resultVar" "$scale[0]"
        	}
        	"Sine"
        	{
           		"sineperiod" 5
            		"timeoffset" "$offset"	       
		"sinemin" "0.85"
		"sinemax" "1.15"
            		"resultVar" "$scale[1]"
        	}

	"LinearRamp"
	{
		"rate" 20
		"initialValue" $offset
		"resultVar" "$angle"
	}

	"LinearRamp"
	{
		"rate" 1
		"initialValue" $offset
		"resultVar" "$translate[0]"
	}

        	"TextureTransform"
        	{
            		"translateVar" "$translate"
            		"rotateVar" "$angle"
            		"centerVar" "$center"
		"scaleVar" "$scale"
            		"resultVar" "$basetexturetransform"
        	}

    }	
}
